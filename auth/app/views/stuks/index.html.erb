<p id="notice"><%= notice %></p>

<h1>Stuks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stuks.each do |stuk| %>
      <tr>
        <td><%= link_to 'Show', stuk %></td>
        <td><%= link_to 'Edit', edit_stuk_path(stuk) %></td>
        <td><%= link_to 'Destroy', stuk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stuk', new_stuk_path %>
